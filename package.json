{
  "scripts": {
    "start": "expo start --dev-client",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "postinstall": "serenity-bdd update",
    "clean": "rimraf target && rm -fr reporter/*.html && rm -fr reporter/json rm -fr reporter/*json",
    "test": "failsafe clean test:execute test:report",
    "test:ios": "wdio config/wdio.ios.app.conf.ts",
    "test:android": "wdio config/wdio.android.app.conf.ts",
    "test:report": "serenity-bdd run --features ./tests/features",
    "ios.app": "wdio config/wdio.ios.app.conf.ts",
    "android.app": "wdio config/wdio.android.app.conf.ts",
    "android.bs.app": "wdio config/wdio.android.app.conf.bs.ts",
    "lint": "eslint --ext ts --config ./tests/eslintrc.js .",
    "lint:fix": "npm run lint -- --fix",
    "report:cucumber": "node reporter/index.js && open reporter/cucumber_report.html",
    "report": "open tests/target/site/serenity/index.html"
  },
  "dependencies": {
    "expo": "~42.0.1",
    "expo-splash-screen": "~0.11.2",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.8.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "~0.63.4",
    "react-native-config": "^1.4.5",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "~3.4.0",
    "react-native-unimodules": "0.14.10",
    "react-native-web": "~0.13.12"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.15.6",
    "@cucumber/cucumber": "^7.3.1",
    "@serenity-js/assertions": "^2.32.3",
    "@serenity-js/console-reporter": "^2.32.3",
    "@serenity-js/core": "^2.32.3",
    "@serenity-js/cucumber": "^2.32.3",
    "@serenity-js/serenity-bdd": "^2.32.3",
    "@serenity-js/webdriverio": "^2.32.3",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@wdio/appium-service": "^7.8.0",
    "@wdio/cli": "^7.11.0",
    "@wdio/cucumber-framework": "^7.9.0",
    "@wdio/local-runner": "^7.9.0",
    "@wdio/spec-reporter": "^7.9.0",
    "browserstack-local": "^1.4.5",
    "chromedriver": "^95.0.0",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^38.0.1",
    "eslint-plugin-unused-imports": "^1.1.5",
    "expect-webdriverio": "^3.1.2",
    "is-ci": "^3.0.0",
    "npm-failsafe": "^0.4.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "wdio-chromedriver-service": "^7.2.0",
    "webdriverio": "^7.13.2",
